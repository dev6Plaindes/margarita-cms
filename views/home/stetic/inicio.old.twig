{% if bienvenido %}
    {% set capacitados = [] %} 
    {% set caracterisitca = [] %} 
    {% set banner = [] %} 
    
    {% for item in bienvenido %}
        {% if item.content_alias == 'banner' %}
            {% set banner = banner|merge([item]) %} 
        {% endif %}

        {% if item.content_alias == 'caracterisitca' %}
            {% set caracterisitca = caracterisitca|merge([item]) %} 
        {% endif %}

        {% if item.content_alias == 'capacitados' %}
            {% set capacitados = capacitados|merge([item]) %} 
        {% endif %}
            
    {% endfor %}
