                </ul>
            </div><!-- /.navbar-collapse -->
            {% if _company.whatsapp_link %}
            <div class="right-side-box">
                <a class="mailchimp-one__icon__header" href="{{ _company.whatsapp_link }}" target="_blank" style="">
                    <i class="fab fa-whatsapp"></i>
                    <!-- /.kipso-icon-magnifying-glass --></a>
