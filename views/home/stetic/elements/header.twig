<header class="site-header site-header__header-one">
    <nav class="navbar navbar-expand-lg navbar-light header-navigation stricky">
        <div class="container clearfix">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="logo-box clearfix">

                <a class="navbar-brand" href="/">
                    <img src="{{ _company.logos[0].patch }}" class="main-logo" width="190" alt="Awesome Image" />
                </a>
           
                {#<a class="navbar-brand" href="/">
                    <img src="/assets/template/stetic/images/logo.png" class="main-logo" width="190" alt="Awesome Image" />
                </a>#}
                <div class="header__social">
                    {% if _company.facebook_link %}<a href="{{ _company.facebook_link }}" target="_blank"><i class="fab fa-facebook-square"></i></a>{% endif %}
                    {% if _company.twitter_link %}<a href="{{ _company.twitter_link }}" target="_blank"><i class="fab fa-twitter"></i></a>{% endif %}
                    {% if _company.linkedin_link %}<a href="{{ _company.linkedin_link }}" target="_blank"><i class="fab fa-linkedin"></i></a>{% endif %}
                    {% if _company.instagram_link %}<a href="{{ _company.instagram_link }}" target="_blank"><i class="fab fa-instagram"></i></a>{% endif %}
                
                </div><!-- /.header__social -->
                <button class="menu-toggler" data-target=".main-navigation">
                    <span class="kipso-icon-menu"></span>
                </button>
            </div><!-- /.logo-box -->
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="main-navigation">
                <ul class=" navigation-box">
                    {% for menu_item in _menu %}
                    {% if menu_item.template_view.index == 2 or menu_item.template_view.index == 4 %}
                    <li class="current">
                        {% if menu_item.children[0] %}
                        <a style="cursor: pointer;">{{ menu_item.name }}</a>
                        {% else %}
                        <a href="/{{ menu_item.slug }}">{{ menu_item.name }}</a>
                        {% endif %}
                        
                        {% if menu_item.children %}
                        <ul class="sub-menu">
                            {% for sub_menu_item in menu_item.children %}
                            <li><a href="/{{ sub_menu_item.slug }}">{{ sub_menu_item.name }}</a>
                                {% if sub_menu_item.children %}
                                <ul class="sub-menu">
                                    {% for sub2_menu_item in sub_menu_item.children %}
                                    <li><a href="/{{ sub2_menu_item.slug }}">{{ sub2_menu_item.name }}</a></li>
                                    {% endfor %}
                                </ul><!-- /.sub-menu -->
                                {% endif %}
                            </li>
                            {% endfor %}
                            {% if menu_item.children[1] %}
                            <li class="current">
                                <a href="/{{ menu_item.slug }}">Ver todos</a>
                            </li>
                            {% endif %}
                        </ul><!-- /.sub-menu -->
                        {% endif %}
                    </li>
                    {% endif %}
                    {% endfor %}
                    {#<li class="current">
                        <a href="./">Nosotros</a>
