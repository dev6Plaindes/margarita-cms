<section class="padding-top-65">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h2 class="title ">{{ noticias[0].section_name|raw }}</h2><span class="seperator"></span>
                <a href="{{ noticias[0].section_caption|raw  }}" target="_blank" class="more-all">{{ noticias[0].section_caption ? 'Ver m√°s':'' }}</a>
                <div class="row clearfix">
                    {% for noticia in noticias %}
                    <article class=" col-sm-6 col-md-6 item-lastest-news">
                        <a href="{{noticia.content_url}}" target="_blank" class="img-news-container">
                            <img src="{{ noticia.imagens[0].patch }}"
                                class="img-responsive" alt="Image">
                        </a>
                        <div class="news-text-container">
                            <a href="{{noticia.content_url}}" target="_blank">
                                <h3 class="title-news">{{noticia.content_name|raw }}</h3>
                            </a>
                            <p>{{noticia.content_description|raw }}</p>
                            <div class="latest-news-data">
                                <span class="tags">
                                    <a href="#">{{ noticia.content_caption|raw }}</a>
                                </span>
                                {#<span class="dates">{{ noticia.content_fecha_update|date("M d, Y") }}</span>#}
                            </div>
                        </div>
                    </article>
